{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "types": [
      "cypress"
    ],
    "useDefineForClassFields": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "modules/*": [
        "src/modules/*"
      ],
      "config/*": [
        "config/*"
      ],
      "app/*": [
        "*"
      ]
    }
  }
}