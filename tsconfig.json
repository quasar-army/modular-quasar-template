{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "cypress"
    ],
    "useDefineForClassFields": true,
    "experimentalDecorators": true,
    "paths": {
      "modules/*": [
        "src/modules/*"
      ],
      "config/*": [
        "config/*"
      ],
      "app/*": [
        "*"
      ]
    }
  },
  "exclude": [
    "./dist",
    "./.quasar",
    "./node_modules",
    "./src-capacitor",
    "./src-cordova",
    "./quasar.config.*.temporary.compiled*"
  ]
}