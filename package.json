{
  "name": "modular-quasar-starter-template",
  "version": "0.0.1",
  "description": "An opinionated starter template for Quasar that uses a modular approach",
  "productName": "ModularQuasarTemplate",
  "author": "luke.diebold@sourcedigital.net",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "pnpm test:component --browser chrome",
    "dev": "cross-env NODE_ENV=local quasar dev -m spa",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "generate": "bun ./scripts/generate.ts"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vuemodel/core": "^0.0.19",
    "@vuemodel/pinia-local-storage": "^0.0.20",
    "@vueuse/core": "^10.7.0",
    "ajv": "^8.12.0",
    "augment-quasar": "^0.0.34",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-orm": "^1.7.2",
    "pluralize": "^8.0.0",
    "quasar": "^2.14.1",
    "unplugin-auto-import": "^0.17.2",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue3-ajv-form": "^0.0.36"
  },
  "devDependencies": {
    "@quasar-army/ts-generator": "^0.0.2",
    "@quasar-army/ts-generator-in-json-schema": "^0.0.2",
    "@quasar-army/ts-generator-out-pinia-orm": "^0.0.4",
    "@quasar-army/ts-generator-persister-file": "^0.0.2",
    "@quasar/app-vite": "^1.7.1",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@unocss/preset-uno": "^0.58.0",
    "@vue-macros/volar": "^0.18.0",
    "autoprefixer": "^10.4.16",
    "change-case": "^5.3.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.19.2",
    "fast-glob": "^3.3.2",
    "inflection": "^3.0.0",
    "postcss": "^8.4.32",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unplugin-vue-macros": "^2.7.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
