{
  "name": "modular-quasar-starter-template",
  "version": "0.0.1",
  "description": "An opinionated starter template for Quasar that uses a modular approach",
  "productName": "ModularQuasarTemplate",
  "author": "luke.diebold@sourcedigital.net",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "pnpm test:component --browser chrome",
    "build": "quasar build -m spa",
    "dev": "cross-env NODE_ENV=local quasar dev -m spa",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "generate": "bun ./scripts/generate.ts",
    "blue:listen": "blue listen"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@vuemodel/core": "^0.0.22",
    "@vuemodel/pinia-local-storage": "^0.0.22",
    "@vueuse/core": "^10.7.2",
    "ajv": "^8.12.0",
    "augment-quasar": "^0.0.35",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-orm": "^1.7.2",
    "pluralize": "^8.0.0",
    "quasar": "^2.14.2",
    "unplugin-auto-import": "^0.17.3",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5",
    "vue3-ajv-form": "^0.0.39"
  },
  "devDependencies": {
    "@quasar-army/ts-generator": "^0.0.2",
    "@quasar-army/ts-generator-in-json-schema": "^0.0.2",
    "@quasar-army/ts-generator-out-pinia-orm": "^0.0.5",
    "@quasar-army/ts-generator-persister-file": "^0.0.2",
    "@quasar/app-vite": "^1.7.1",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.3",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@unocss/preset-uno": "^0.58.3",
    "@vue-macros/volar": "^0.18.10",
    "autoprefixer": "^10.4.16",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.20.1",
    "fast-glob": "^3.3.2",
    "inflection": "^3.0.0",
    "postcss": "^8.4.33",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-vue-macros": "^2.7.9",
    "dotenv": "^16.4.1"
  },
  "engines": {
    "node": "^20 || ^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}