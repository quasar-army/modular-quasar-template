{
  "name": "modular-quasar-starter-template",
  "type": "module",
  "version": "0.0.1",
  "description": "An opinionated starter template for Quasar that uses a modular approach",
  "productName": "ModularQuasarTemplate",
  "author": "luke.diebold@sourcedigital.net",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "pnpm test:component --browser chrome",
    "build": "quasar build -m spa",
    "dev": "cross-env NODE_ENV=local quasar dev -m spa",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "generate": "bun ./scripts/generate.ts",
    "blue:listen": "blue listen"
  },
  "dependencies": {
    "@augment-quasar/core": "^0.0.6",
    "@quasar/extras": "^1.16.11",
    "@vuemodel/core": "^0.0.36",
    "@vuemodel/pinia-local-storage": "^0.0.25",
    "@vueuse/core": "^10.10.0",
    "ajv": "^8.14.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-orm": "^1.9.1",
    "pluralize": "^8.0.0",
    "quasar": "^2.16.4",
    "unplugin-auto-import": "^0.17.6",
    "vite-plugin-checker": "^0.6.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "vue-tsc": "^1.8.27",
    "vue3-ajv-form": "^0.0.43"
  },
  "devDependencies": {
    "@quasar-army/blue-cli": "^0.0.15",
    "@quasar-army/ts-generator": "^0.0.2",
    "@quasar-army/ts-generator-in-json-schema": "^0.0.5",
    "@quasar-army/ts-generator-out-pinia-orm": "^0.0.8",
    "@quasar-army/ts-generator-persister-file": "^0.0.2",
    "@quasar/app-vite": "^2.0.0-beta.14",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@unocss/preset-uno": "^0.60.3",
    "autoprefixer": "^10.4.19",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "fast-glob": "^3.3.2",
    "inflection": "^3.0.0",
    "typescript": "^5.4.5",
    "unocss": "^0.58.9"
  },
  "packageManager": "pnpm@9.1.3+sha256.7f63001edc077f1cff96cacba901f350796287a2800dfa83fe898f94183e4f5f",
  "engines": {
    "node": "^20 || ^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}